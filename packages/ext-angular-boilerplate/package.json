{
  "name": "@sencha/ext-angular-boilerplate",
  "version": "7.0.0",
  "description": "ext-angular-boilerplate",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "b1": "npm run clean && cross-env webpack --env.environment=production --env.treeshake=yes",
    "b2": "cross-env webpack --env.environment=production --env.basehref=/",
    "build": "npm run clean && cross-env webpack --env.environment=production --env.treeshake=yes && cross-env webpack --env.environment=production --env.basehref=/ && npm run gzip && npm run gzip2 && npm run replace",
    "gzip": "npx gzip build/*.js",
    "gzip2": "npx gzip build/ext/*.js",
    "replace": "npx replace-in-file .js .js.gz build/index.html"
},
  "devDependencies": {
    "@sencha/ext-angular": "~7.0.0",
    "@sencha/ext-webpack-plugin": "~7.0.0",
    "@sencha/ext": "~7.0.0",
    "@sencha/ext-modern": "~7.0.0",
    "@sencha/ext-modern-treegrid": "~7.0.0",
    "@sencha/ext-modern-theme-material": "~7.0.0",
    "@sencha/ext-ux": "~7.0.0",

    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ngtools/webpack":"~8.1.3",

    "core-js": "~3.1.4",
    "zone.js": "~0.10.0",

    "typescript": "3.5.3",
    "@types/core-js": "~2.5.2",
    "@types/node": "~12.6.8",

    "rimraf": "~2.6.3",
    "cross-env": "~5.2.0",
    "portfinder": "~1.0.21",
    "gzip-cli": "^1.0.1",
    "replace-in-file": "^4.1.1",

    "css-loader": "~3.1.0",
    "file-loader":"~4.1.0",
    "html-loader": "~0.5.5",
    "style-loader": "~0.23.1",
    "to-string-loader": "~1.1.5",

    "html-webpack-plugin": "~3.2.0",
    "base-href-webpack-plugin": "~2.0.0",
    "webpack-filter-warnings-plugin": "~1.2.1",

    "webpack": "~4.38.0",
    "webpack-cli": "~3.3.6",
    "webpack-dev-server": "~3.7.2"
  },
