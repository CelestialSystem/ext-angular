{
  "name": "@sencha/ext-angular-boilerplate",
  "version": "7.0.0",
  "description": "ext-angular-boilerplate",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "build": "npm run clean && npm run webpack1 && npm run gzip && npm run http",
    "clean": "rimraf build",
    "xwebpack": "npm run webpack1 && npm run webpack2",
    "webpack1": "cross-env webpack --env.environment=production --env.treeshake=yes --env.verbose=no",
    "xwebpack2": "cross-env webpack --env.environment=production --env.treeshake=no --env.basehref=/",
    "gzip": "npm run gzipbuild && npm run gzipext",
    "gzipbuild": "npx gzip build/*.js",
    "gzipext": "npx gzip build/ext/*.js",
    "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
    "http": "npx http-server build -g -o"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@sencha/ext-angular": "~7.0.0",
    "@sencha/ext-webpack-plugin": "~7.0.0",
    "@sencha/ext": "~7.0.0",
    "@sencha/ext-modern": "~7.0.0",
    "@sencha/ext-modern-treegrid": "~7.0.0",
    "@sencha/ext-modern-theme-material": "~7.0.0",
    "@sencha/ext-ux": "~7.0.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ngtools/webpack": "~8.2.0",
    "core-js": "~3.1.4",
    "zone.js": "~0.10.1",
    "typescript": "3.5.3",
    "@types/core-js": "~2.5.2",
    "@types/node": "~12.7.0",
    "rimraf": "~2.6.3",
    "cross-env": "~5.2.0",
    "portfinder": "~1.0.21",
    "gzip-cli": "^1.0.1",
    "replace-in-file": "^4.1.2",
    "http-server": "^0.11.1",
    "css-loader": "~3.2.0",
    "file-loader": "~4.1.0",
    "html-loader": "~0.5.5",
    "style-loader": "~1.0.0",
    "to-string-loader": "~1.1.5",
    "html-webpack-plugin": "~3.2.0",
    "base-href-webpack-plugin": "~2.0.0",
    "webpack-filter-warnings-plugin": "~1.2.1",
    "webpack": "~4.39.1",
    "webpack-cli": "~3.3.6",
    "webpack-dev-server": "~3.7.2"
  },
